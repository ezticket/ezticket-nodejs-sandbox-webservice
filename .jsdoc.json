{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            ".",
            "contracts/"
        ],
        "exclude": [
            "node_modules"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/docdash"
    }
}